<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="SearchInPage" verbose="1">
    <test name="Search">
        <groups>
            <run>
                <include name="positive"/>
                <include name="negative"/>
            </run>
        </groups>
        <classes>
            <class name="PageObject.MicrosoftTest">
                <methods>
                    <include name="testSearch"/>
                </methods>
            </class>
            <class name = "PageObject.MicrosoftTest">
                <methods>
                    <include name ="negTestSearch"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>